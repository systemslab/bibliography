	date-modified = {2020-01-04 21:52:42 -0700},
	keywords = {papers, erasurecodes, performance, flash, garbagecollection, predictable},
	month = {October 5},
	title = {Erasure Coding \& Read/Write Separation in Flash Storage},
	year = {2014},
